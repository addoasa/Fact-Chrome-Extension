{
    "name": "Chuck Clipboard",
    "version": "1.0",
    "permissions": ["declarativeContent","storage"],
    "description": "Build an Extension!",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
    "options_page": "options.html",
    "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/chuck16.png",
      "32": "images/chuck32.png",
      "48": "images/chuck48.png",
      "128": "images/chuck128.png"
      }
    },
    "icons": {
        "16": "images/chuck16.png",
        "32": "images/chuck32.png",
        "48": "images/chuck48.png",
        "128": "images/chuck128.png"
      },
    "manifest_version": 2
}